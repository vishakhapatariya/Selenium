<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
	<listeners>
		<listener class-name="Selenium.E2EProject.Listeners"></listener>
	</listeners>
	<test thread-count="5" name="Validating Login Functionality">
		<classes>
			<class name="Selenium.E2EProject.ValidateUserLogin" />
		</classes>
	</test>
	<test thread-count="5" name="Validating Navigationbar in HomePage">
		<classes>
			<class name="Selenium.E2EProject.ValidateNavigationbar" />
		</classes>
	</test>
	<test thread-count="5" name="Validating title in HomePage">
		<classes>
			<class name="Selenium.E2EProject.ValidateTitleHome" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->
